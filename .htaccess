RewriteEngine on
RewriteRule  ^([a-z0-9\-_]+)/([a-z0-9\-_]+)-([0-9]+).([0-9]+)/tap-([A-Za-z0-9\-_]+)/$  index.php?action=$1&ten_film=$2&id=$3&id_dm=$4&tap=$5
#?action=thong-tin-phim&id=11&id_dm=2&tap=1

RewriteRule  ^([a-z0-9\-_]+)/([a-z0-9\-_]+)-([0-9]+)/page-([0-9]+)/$  index.php?action=$1&ten=$2&id=$3&page=$4

RewriteRule  ^([a-zA-Z-_]+)/page-([0-9]+)/$  index.php?action=$1&page=$2

RewriteRule ^([a-z-]+)/$  index.php?action=$1

RewriteRule ^([a-z-]+)-([0-9]+)/$  index.php?action=$1&id=$2

#RewriteRule ^([a-z]+)-([a-z]+)/$  index.php?controller=$1&action=$2

